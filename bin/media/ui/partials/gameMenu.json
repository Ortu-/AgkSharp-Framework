[
	{"type": "styleClass",
		"name": "btn-text",
		"style": {
			"width": "250px",
			"height": "50px",
			"padding-left": "6%",
			"text-alignV": "center",
			"background-opacity": "0",
			"font-size": "32",
			"z-index": "5"
		}
	},
	{
		"type": "element",
		"tag": "div",
		"id": "gameMenu",
		"holdMouseFocus": "true",
		"holdPause": "true",
		"enableEvents": "2",
		"keyBind": "27",
		"onPress": "GameMenuController.MenuOnPress",
		"style": {
			"width": "100%",
			"height": "100%",
			"display": "hidden"
		},
		"children": [
			{
				"type": "element",
				"tag": "button",
				"id": "btn-resume",
				"styleClass": "btn-text",
				"name": "btn-txt",
				"value": "Resume",
				"enableEvents": "1",
				"onPress": "GameMenuController.ResumeOnPress",
				"style": {
					"position-alignV": "center",
					"top": "-30px"
				}
			},
			{
				"type": "element",
				"tag": "button",
				"id": "btn-close",
				"styleClass": "btn-text",
				"name": "btn-txt",
				"value": "Quit",
				"enableEvents": "1",
				"onPress": "GameMenuController.QuitOnPress",
				"style": {
					"position-alignV": "center",
					"top": "30px"
				}
			}
		]
	}
]